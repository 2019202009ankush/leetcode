class Solution {
public:
    int findComplement(int n) {
      long int num=n;
        long int sum=0;
        long int i=log(n)/log(2);
        if(pow(2,i)==n) i--;
        
      for(;i>=0;i--)
      {
          if(num>>i&1);
          else
          {
            sum+=(pow(2,i));
          }
      }
        return sum;
    }
};